{
    "name": "tps/paypal-bundle",
    "description": "Symfony2 wrapper-bundle for rest-api-sdk-php and classic-api from paypal",
    "keywords": ["paypal", "rest", "classic"],
    "homepage": "https://github.com/leberknecht/tpsPaypalBundle",
    "require": {
        "php":  ">=5.3.2",
        "symfony/framework-bundle": ">=2.0.16",
        "paypal/rest-api-sdk-php": "v0.7.1",
        "paypal/merchant-sdk-php": "v3.6.106"
    },

    "autoload": {
        "psr-0": { "tps\\PaypalBundle": "" }
    },
    "target-dir": "tps/PaypalBundle"
}
